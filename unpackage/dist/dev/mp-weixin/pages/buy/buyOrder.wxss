.shortShadow {
  position: fixed;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  background: rgba(0, 0, 0, 0.6);
  z-index: 999;
}
.shortShadow .toastBox {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  -webkit-transform: translate(-50%, -50%);
  -moz-transform: translate(-50%, -50%);
  border-radius: 10rpx;
  padding: 20rpx;
  background-image: url("https://xcxsite.vjifen.com/v/shanDongImgs/mall/toastBg.png");
  background-repeat: no-repeat;
  background-position: 0;
  background-size: 100% 100%;
  position: relative;
  color: #fff;
  width: 551rpx;
  height: 442rpx;
}
.shortShadow .toastBox .close {
  width: 49rpx;
  position: absolute;
  top: -10%;
  right: -8%;
  display: block;
}
.shortShadow .toastBox .toastContent {
  text-align: center;
}
.shortShadow .toastBox .toastContent .title {
  font-size: 38rpx;
  font-weight: 400;
  color: #fff;
  line-height: 80rpx;
  text-align: center;
  padding-bottom: 115rpx;
}
