  -webkit-justify-content: space-between;
          justify-content: space-between;
}
.add_page .add_msg .item text:nth-of-type(1) {
  font-size: 30rpx;
  color: #333;
  line-height: 110rpx;
}
.add_page .add_msg .item.area {
  border-bottom: none;
}
.add_page .add_msg .item.area .select {
