.add_page {
  height: 100%;
  background: #F8F8F8;
  overflow: hidden;
}
.add_page .add_msg {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-orient: vertical;
  -webkit-box-direction: normal;
  -webkit-flex-direction: column;
          flex-direction: column;
  -webkit-box-pack: justify;
